{
  "name": "@codecarvalho/aws-helper",
  "version": "0.0.0",
  "description": "",
  "private": false,
  "repository": "https://github.com/direisc/aws-helper",
  "keywords": [
    "aws-helper",
    "helper",
    "aws"
  ],
  "author": "Diego Reis Carvalho",
  "license": "MIT",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "npx prettier --write .",
    "lint": "npx eslint --fix . --ext .js,.ts",
    "prepare" : "npm run build",
    "prepublishOnly" : "npm test && npm run lint",
    "preversion" : "npm run lint",
    "version" : "npm run format && git add -A src",
    "postversion" : "git push && git push --tags"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "sqs": {
      "types": "./dist/sqs/index.d.ts",
      "default": "./dist/sqs/index.ts"
    }
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.113",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.3",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.55.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.7.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.4",
    "ts-jest": "^29.0.5",
    "typescript": "^5.0.2"
  }
}
